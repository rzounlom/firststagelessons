{
  "name": "firststagelessons",
  "version": "1.0.0",
  "description": "Company Website for First Stage Guitar Lessons",
  "main": "index.js",
  "scripts": {
    "client": "npm start --prefix client",
    "start": "node src/index.js",
    "server": "nodemon src/index.js",
