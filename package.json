  "bugs": {
    "url": "https://github.com/rzounlom/firststagelessons/issues"
  },
  "homepage": "https://github.com/rzounlom/firststagelessons#readme",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
